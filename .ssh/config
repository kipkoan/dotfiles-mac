Host *
  GSSAPIAuthentication no
  GSSAPIKeyExchange no
  AddressFamily inet
  TCPKeepAlive yes
  ServerAliveInterval 10
  ControlMaster auto
  ControlPath /tmp/ssh_mux_%r@%h:%p
  ControlPersist 10m
  StrictHostKeyChecking=no

  # Forward Clipper connection to remote host
  RemoteForward 8377 localhost:8377
